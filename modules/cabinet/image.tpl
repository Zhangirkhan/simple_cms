<li class="col-xs-6 col-sm-4 col-md-3" data-responsive="{IMAGE_URL}" data-src="{IMAGE_URL}" data-sub-html="{IMAGE_TITLE}">
    <div class="card h-200">
        <div class="card-header">
            <p class="card-title" style="font-size: 10px">{IMAGE_TITLE}</p>
            <div class="card-options">
                <a href="#" class="card-options-remove" data-toggle="card-remove" onclick="deleteImage({IMAGE_ID});"><i class="fe fe-x"></i></a>
            </div>
        </div>
        <a href="" style="height: 200px;">
            <img class="img-responsive " src="{IMAGE_URL}" alt="Thumb-1">
        </a>
    </div>
</li>